
if __name__ == '__main__':
    n = int(input().strip())
    match n:
        case n if n % 2 == 1:
            print("Weird")
        case n if n % 2 == 0 and 2 <= n <=5:
            print("Not Weird")
        case n if n % 2 == 0 and 6 <= n <= 20:
            print("Weird")
        case n if n % 2 == 0 and n > 20:
            print("Not Weird")
